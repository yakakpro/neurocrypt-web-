          alert("–ù–µ–º–∞—î –µ–ª–µ–º–µ–Ω—Ç—ñ–≤ –¥–ª—è —Å—Ç–≤–æ—Ä–µ–Ω–Ω—è —Ç—Ä–∞–Ω—Å—É–ª—Ç—Ä–∞–≥—ñ–ø–µ—Ä—Ç—Ä–µ–Ω—Å–∞–±—Å–æ–ª—é—Ç–Ω–æ—ó –æ–º–Ω—ñ–≥—ñ–ø–µ—Ä—É–ª—å—Ç—Ä–∞–≥—ñ–ø–µ—Ä—É–ª—å—Ç—Ä–∞–≥—ñ–ø–µ—Ä–æ–º–Ω—ñ–≥–∞—Ä–º–æ–Ω—ñ—ó!");
          return;
        }

        // Apply entropy and transultrahypertransabsolute omnihyperultrahyperultrahyperomniharmony level
        elements.forEach(element => {
          if (Math.random() < entropy) {
            element.resonance = Math.max(0, Math.min(100, element.resonance + (Math.random() * 20 - 10)));
          }
          element.resonance = Math.min(100, element.resonance * (1 + transultrahypertransabsoluteOmnihyperultrahyperultrahyperomniharmonyLevel / 10));
        });

        omnihyperultrahyperultrahyperomniharmonyRunning = true;
        const omnihyperultrahyperultrahyperomniharmonyData = {
          elements,
          moments,
          entropy,
          transultrahypertransabsoluteOmnihyperultrahyperultrahyperomniharmonyLevel,
          fusionType,
          timestamp: new Date().toISOString()
        };

        const kyber = new Kyber.Kyber512();
        const { publicKey, secretKey } = await kyber.keygen();
        const encrypted = await kyber.encrypt(publicKey, JSON.stringify(omnihyperultrahyperultrahyperomniharmonyData));
        const encryptedStr = btoa(String.fromCharCode(...encrypted));
        const signature = await signData(encryptedStr);
        const finalOmnihyperultrahyperultrahyperomniharmony = { encrypted: encryptedStr, signature, publicKey: btoa(String.fromCharCode(...publicKey)) };
        localStorage.setItem("resonanceTransultrahypertransabsoluteOmnihyperultrahyperultrahyperomniharmony", JSON.stringify(finalOmnihyperultrahyperultrahyperomniharmony));

        displayElements(elements);
        document.getElementById("transultrahypertransabsolute-omnihyperultrahyperultrahyperomniharmony-data").textContent = JSON.stringify(finalOmnihyperultrahyperultrahyperomniharmony, null, 2);
        document.getElementById("transultrahypertransabsolute-omnihyperultrahyperultrahyperomniharmony-data").style.display = "block";
        document.getElementById("saveTransultrahypertransabsoluteOmnihyperultrahyperultrahyperomniharmony").style.display = "inline-block";
        document.getElementById("savePng").style.display = "inline-block";
