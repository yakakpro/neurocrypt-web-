    function saveMoment() {
      if (!omnihyperultrahyperultrahyperomniharmonyRunning) {
        alert("–°–ø–æ—á–∞—Ç–∫—É –∑–∞–ø—É—Å—Ç–∏ —Ç—Ä–∞–Ω—Å—É–ª—Ç—Ä–∞–≥—ñ–ø–µ—Ä—Ç—Ä–µ–Ω—Å–∞–±—Å–æ–ª—é—Ç–Ω—É –æ–º–Ω—ñ–≥—ñ–ø–µ—Ä—É–ª—å—Ç—Ä–∞–≥—ñ–ø–µ—Ä—É–ª—å—Ç—Ä–∞–≥—ñ–ø–µ—Ä–æ–º–Ω—ñ–≥–∞—Ä–º–æ–Ω—ñ—é!");
        return;
      }
      const moment = {
        elements: elements.map(el => ({ type: el.type, name: el.name || el.description || el.singularities?.length || el.elements?.length, resonance: el.resonance })),
        timestamp: new Date().toISOString()
      };
      moments.push(moment);
      document.getElementById("transultrahypertransabsolute-omnihyperultrahyperultrahyperomniharmony").innerHTML += `<br>üìç –ú–æ–º–µ–Ω—Ç –∑–±–µ—Ä–µ–∂–µ–Ω–æ: ${moment.timestamp}`;
    }

    function stopTransultrahypertransabsoluteOmnihyperultrahyperultrahyperomniharmony() {
      omnihyperultrahyperultrahyperomniharmonyRunning = false;
      document.getElementById("transultrahypertransabsolute-omnihyperultrahyperultrahyperomniharmony").innerHTML += "<br>‚èπÔ∏è –¢—Ä–∞–Ω—Å—É–ª—Ç—Ä–∞–≥—ñ–ø–µ—Ä—Ç—Ä–µ–Ω—Å–∞–±—Å–æ–ª—é—Ç–Ω—É –æ–º–Ω—ñ–≥—ñ–ø–µ—Ä—É–ª—å—Ç—Ä–∞–≥—ñ–ø–µ—Ä—É–ª—å—Ç—Ä–∞–≥—ñ–ø–µ—Ä–æ–º–Ω—ñ–≥–∞—Ä–º–æ–Ω—ñ—é –∑—É–ø–∏–Ω–µ–Ω–æ!";
    }

    function saveTransultrahypertransabsoluteOmnihyperultrahyperultrahyperomniharmony() {
      if (!blobCache) {
        blobCache = new Blob([JSON.stringify({ elements, moments }, null, 2)], {type: "application/json"});
      }
      const a = document.createElement("a");
      a.href = URL.createObjectURL(blobCache);
      a.download = "resonance-transultrahypertransabsolute-omnihyperultrahyperultrahyperomniharmony.json";
      a.click();
    }

    function savePng() {
      const a = document.createElement("a");
      a.href = omnihyperultrahyperultrahyperomniharmonyCanvas.toDataURL("image/png");
      a.download = "resonance-transultrahypertransabsolute-omnihyperultrahyperultrahyperomniharmony.png";
      a.click();
    }

    let blobCache = null;
  </script>
</body>
</html>
