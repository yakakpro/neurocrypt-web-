        alert("–ü–æ–º–∏–ª–∫–∞ –∑–∞–ø—É—Å–∫—É —Ç—Ä–∞–Ω—Å—É–ª—Ç—Ä–∞–≥—ñ–ø–µ—Ä—Ç—Ä–µ–Ω—Å–∞–±—Å–æ–ª—é—Ç–Ω–æ—ó –æ–º–Ω—ñ–≥—ñ–ø–µ—Ä—É–ª—å—Ç—Ä–∞–≥—ñ–ø–µ—Ä—É–ª—å—Ç—Ä–∞–≥—ñ–ø–µ—Ä–æ–º–Ω—ñ–≥–∞—Ä–º–æ–Ω—ñ—ó: " + e.message);
      }
    }

    function displayElements(displayElements) {
      let output = `üåå –ï–ª–µ–º–µ–Ω—Ç—ñ–≤ —É —Ç—Ä–∞–Ω—Å—É–ª—Ç—Ä–∞–≥—ñ–ø–µ—Ä—Ç—Ä–µ–Ω—Å–∞–±—Å–æ–ª—é—Ç–Ω—ñ–π –æ–º–Ω—ñ–≥—ñ–ø–µ—Ä—É–ª—å—Ç—Ä–∞–≥—ñ–ø–µ—Ä—É–ª—å—Ç—Ä–∞–≥—ñ–ø–µ—Ä–æ–º–Ω—ñ–≥–∞—Ä–º–æ–Ω—ñ—ó: ${displayElements.length}<br>`;
      displayElements.forEach((element, i) => {
        output += `<div class="element">`;
        output += `üìç –ï–ª–µ–º–µ–Ω—Ç ${i + 1}: ${element.name || element.description || element.singularities?.length || element.elements?.length || "–Ω–µ–≤—ñ–¥–æ–º–æ"}<br>`;
        if (element.type === "glyph") {
          output += `üß¨ –ì–ª—ñ—Ñ: ${element.glyph.join(" + ")}<br>`;
          output += `üîó –†–µ–∑–æ–Ω–∞–Ω—Å: ${element.resonance.toFixed(1)}%`;
