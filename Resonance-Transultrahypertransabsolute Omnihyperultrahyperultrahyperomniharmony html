      }
    });

    async function verifySignature(data, signature) {
      const encoder = new TextEncoder();
      const computed = btoa(String.fromCharCode(...encoder.encode(data))).slice(0, 32);
      return computed === signature;
    }

    async function signData(data) {
      const encoder = new TextEncoder();
      const hash = btoa(String.fromCharCode(...encoder.encode(data)));
      return hash.slice(0, 32);
    }

    async function startTransultrahypertransabsoluteOmnihyperultrahyperultrahyperomniharmony() {
      try {
        const entropy = parseFloat(document.getElementById("entropy-factor").value) || 0.5;
        const transultrahypertransabsoluteOmnihyperultrahyperultrahyperomniharmonyLevel = parseInt(document.getElementById("transultrahypertransabsolute-omnihyperultrahyperultrahyperomniharmony-level").value) || 5;
        const fusionType = document.getElementById("fusion-type").value;
        if (entropy < 0 || entropy > 1) {
          alert("–§–∞–∫—Ç–æ—Ä –µ–Ω—Ç—Ä–æ–ø—ñ—ó –º–∞—î –±—É—Ç–∏ –≤—ñ–¥ 0 –¥–æ 1!");
          return;
        }
        if (transultrahypertransabsoluteOmnihyperultrahyperultrahyperomniharmonyLevel < 0 || transultrahypertransabsoluteOmnihyperultrahyperultrahyperomniharmonyLevel > 10) {
          alert("–û–º–Ω—ñ–≥—ñ–ø–µ—Ä—É–ª—å—Ç—Ä–∞–≥—ñ–ø–µ—Ä—É–ª—å—Ç—Ä–∞–≥—ñ–ø–µ—Ä–æ–º–Ω—ñ–≥–∞—Ä–º–æ–Ω—ñ–π–Ω–∏–π —Ä—ñ–≤–µ–Ω—å –º–∞—î –±—É—Ç–∏ –≤—ñ–¥ 0 –¥–æ 10!");
          return;
        }

        elements = [];
        const sources = [
          { key: "resoglyph", type: "glyph" },
          { key: "resonanceCommunity", type: "glyph" },
          { key: "resonanceMarket", type: "glyph" },
          { key: "resonanceOracle", type: "glyph" },
          { key: "resonanceConstructor", type: "network" },
          { key: "resonanceConnector", type: "network" },
          { key: "resonanceArchitect", type: "rule" },
          { key: "resonanceCosmos", type: "cosmos" },
          { key: "resonanceAbsolute", type: "absolute" },
          { key: "resonanceInfinity", type: "infinity" },
          { key: "resonanceTranscendence", type: "transcendence" },
          { key: "resonanceOmnireality", type: "omnireality" },
          { key: "resonanceUniversalia", type: "universalia" },
          { key: "resonanceTotalia", type: "totalia" },
          { key: "resonanceUltimateUnity", type: "ultimate-unity" },
          { key: "resonanceOversoul", type: "oversoul" },
          { key: "resonanceOmnion", type: "omnion" },
          { key: "resonanceAbsoluteOmnion", type: "absolute-omnion" },
          { key: "resonanceTransdimensionalUnity", type: "transdimensional-unity" },
          { key: "resonanceUltradimensionalUnity", type: "ultradimensional-unity" },
          { key: "resonanceOmnicosmicUnity", type: "omnicosmic-unity" },
          { key: "resonanceAbsoluteHyperreality", type: "absolute-hyperreality" },
          { key: "resonanceTranshyperreality", type: "transhyperreality" },
          { key: "resonanceOmnihyperreality", type: "omnihyperreality" },
          { key: "resonanceUltrahyperreality", type: "ultrahyperreality" },
          { key: "resonanceAbsoluteUltrareality", type: "absolute-ultrareality" },
          { key: "resonanceTranscendentOmnion", type: "transcendent-omnion" },
          { key: "resonanceAbsoluteSingularity", type: "absolute-singularity" },
          { key: "resonanceEternalUnity", type: "eternal-unity" },
          { key: "resonanceUltraeternalHarmony", type: "ultraeternal-harmony" },
          { key: "resonanceAbsoluteCosmoharmony", type: "absolute-cosmoharmony" },
          { key: "resonanceOmniabsoluteHarmony", type: "omniabsolute-harmony" },
          { key: "resonanceTransinfiniteMetaharmony", type: "transinfinite-metaharmony" },
          { key: "resonanceAbsoluteMetaomniharmony", type: "absolute-metaomniharmony" },
          { key: "resonanceOmniultraTranscendentHarmony", type: "omniultra-transcendent-harmony" },
          { key: "resonanceAbsoluteUltraomniharmony", type: "absolute-ultraomniharmony" },
          { key: "resonanceTransabsoluteOmnihyperharmony", type: "transabsolute-omnihyperharmony" },
          { key: "resonanceTranshyperabsoluteHarmony", type: "transhyperabsolute-harmony" },
          { key: "resonanceUltraabsoluteTransharmony", type: "ultraabsolute-transharmony" },
          { key: "resonanceAbsoluteTransultraharmony", type: "absolute-transultraharmony" },
          { key: "resonanceUltrabsoluteHyperharmony", type: "ultrabsolute-hyperharmony" },
          { key: "resonanceHyperabsoluteUltraharmony", type: "hyperabsolute-ultraharmony" },
          { key: "resonanceUltraabsoluteHyperultraharmony", type: "ultraabsolute-hyperultraharmony" },
          { key: "resonanceTransabsoluteOmnihyperultraharmony", type: "transabsolute-omnihyperultraharmony" },
          { key: "resonanceOmniabsoluteUltrahyperomniharmony", type: "omniabsolute-ultrahyperomniharmony" },
          { key: "resonanceUltratransabsoluteOmniultrahyperomniharmony", type: "ultratransabsolute-omniultrahyperomniharmony" },
          { key: "resonanceHypertransabsoluteOmnihyperultrahyperomniharmony", type: "hypertransabsolute-omnihyperultrahyperomniharmony" },
          { key: "resonanceUltrahypertransabsoluteOmniultrahyperultrahyperomniharmony", type: "ultrahypertransabsolute-omniultrahyperultrahyperomniharmony" }
        ];

        for (const source of sources) {
          if (fusionType !== "all" && source.type !== fusionType) continue;
          const data = localStorage.getItem(source.key);
          if (data) {
            const parsed = JSON.parse(data);
            if (Array.isArray(parsed)) {
              parsed.forEach(item => {
                if (item.encrypted && item.signature && verifySignature(item.encrypted, item.signature)) {
                  const eventData = JSON.parse(atob(item.encrypted));
                  if (source.type === "glyph" && eventData.glyph) {
                    elements.push({
                      type: source.type,
                      name: eventData.title || source.key,
                      glyph: eventData.glyph,
                      resonance: eventData.resonance || 60,
                      timestamp: eventData.timestamp || new Date().toISOString()
                    });
                  } else if (source.type === "network" && eventData.nodes) {
                    elements.push({
                      type: source.type,
                      name: source.key,
                      nodes: eventData.nodes,
                      resonance: eventData.nodes.reduce((sum, node) => sum + (node.resonance || 60), 0) / eventData.nodes.length,
                      timestamp: eventData.timestamp || new Date().toISOString()
                    });
                  }
                }
              });
            } else if (parsed.encrypted && parsed.signature && (await verifySignature(parsed.encrypted, parsed.signature))) {
              const eventData = JSON.parse(atob(parsed.encrypted));
              if (source.type === "glyph" && eventData.glyph) {
                elements.push({
                  type: source.type,
                  name: eventData.title || source.key,
                  glyph: eventData.glyph,
                  resonance: eventData.resonance || 60,
                  timestamp: eventData.timestamp || new Date().toISOString()
                });
              } else if (source.type === "network" && eventData.nodes) {
                elements.push({
                  type: source.type,
                  name: source.key,
                  nodes: eventData.nodes,
                  resonance: eventData.nodes.reduce((sum, node) => sum + (node.resonance || node.probability * 100 || 60), 0) / eventData.nodes.length,
                  timestamp: eventData.timestamp || new Date().toISOString()
                });
              } else if (source.type === "rule" && eventData.nodes) {
                elements.push({
                  type: source.type,
                  description: eventData.description,
                  ruleType: eventData.ruleType,
                  threshold: eventData.threshold,
                  entropy: eventData.entropy,
                  resonance: eventData.nodes.reduce((sum, node) => sum + (node.resonance || 60), 0) / eventData.nodes.length,
                  timestamp: eventData.timestamp || new Date().toISOString()
                });
              } else if (source.type === "cosmos" && eventData.singularities) {
                elements.push({
                  type: source.type,
                  singularities: eventData.singularities,
                  resonance: eventData.singularities.reduce((sum, s) => sum + (s.elements.reduce((esum, e) => esum + (e.resonance || 60), 0) / s.elements.length), 0) / eventData.singularities.length,
                  timestamp: eventData.timestamp || new Date().toISOString()
                });
              } else if (source.type === "absolute" && eventData.elements) {
                elements.push({
                  type: source.type,
                  elements: eventData.elements,
                  resonance: eventData.elements.reduce((sum, e) => sum + (e.resonance || 60), 0) / eventData.elements.length,
                  timestamp: eventData.timestamp || new Date().toISOString()
                });
              } else if (source.type === "infinity" && eventData.elements) {
                elements.push({
                  type: source.type,
                  elements: eventData.elements,
                  resonance: eventData.elements.reduce((sum, e) => sum + (e.resonance || 60), 0) / eventData.elements.length,
                  timestamp: eventData.timestamp || new Date().toISOString()
                });
              } else if (source.type === "transcendence" && eventData.elements) {
                elements.push({
                  type: source.type,
                  elements: eventData.elements,
                  resonance: eventData.elements.reduce((sum, e) => sum + (e.resonance || 60), 0) / eventData.elements.length,
                  timestamp: eventData.timestamp || new Date().toISOString()
                });
              } else if (source.type === "omnireality" && eventData.elements) {
                elements.push({
                  type: source.type,
                  elements: eventData.elements,
                  resonance: eventData.elements.reduce((sum, e) => sum + (e.resonance || 60), 0) / eventData.elements.length,
                  timestamp: eventData.timestamp || new Date().toISOString()
                });
              } else if (source.type === "universalia" && eventData.elements) {
                elements.push({
                  type: source.type,
                  elements: eventData.elements,
                  resonance: eventData.elements.reduce((sum, e) => sum + (e.resonance || 60), 0) / eventData.elements.length,
                  timestamp: eventData.timestamp || new Date().toISOString()
                });
              } else if (source.type === "totalia" && eventData.elements) {
                elements.push({
                  type: source.type,
